  "scripts": {
    "ng": "nx",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "npx nx serve server-asset-sg",
    "start:client": "npx nx serve client-asset-sg",
    "start:sync": "npx nx serve sync-asset-sg --watch false",
    "build": "npx nx run-many -t build -p server-asset-sg client-asset-sg sync-asset-sg --configuration=production",
    "build:server": "npx nx build server-asset-sg --configuration=production",
    "build:client": "npx nx build client-asset-sg --configuration=production",
    "api-command": "cd apps/server-asset-sg/ && ts-node -r tsconfig-paths/register --project tsconfig.app.json src/run-command.ts",
    "prisma": "dotenv -e libs/persistence/.env -- npx prisma",
    "test": "npx nx run-many --target=test --all",
    "lint": "npx nx run-many --target=lint --all && npx prettier --check .",
    "lint:fix": "npx nx run-many --target=lint --all -- --fix && npx prettier . --write",
    "cypress:open": "cypress open --config-file cypress/cypress.config.ts",
    "cypress:run": "cypress run --config-file cypress/cypress.config.ts",
    "prepare": "husky"
  },
